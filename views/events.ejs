<!DOCTYPE html>
<!--
This is a starter template page. Use this page to start your new project from
scratch. This page gets rid of all links and provides the needed markup only.
-->
<html>
  <head>
    <meta charset="UTF-8">
    <title>AdminLTE 2 | Dashboard</title>
    <meta content='width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no' name='viewport'>
    <!-- Bootstrap 3.3.4 -->
    <% include commons/css.ejs %>
    <link rel="stylesheet" type="text/css" href="/bootstrap/css/dataTables.bootstrap.css">
    <link href="/bootstrap/css/bootstrap-datetimepicker.min.css" rel="stylesheet" type="text/css" />
  </head>
  <!--
  BODY TAG OPTIONS:
  =================
  Apply one or more of the following classes to get the
  desired effect
  |---------------------------------------------------------|
  | SKINS         | skin-blue                               |
  |               | skin-black                              |
  |               | skin-purple                             |
  |               | skin-yellow                             |
  |               | skin-red                                |
  |               | skin-green                              |
  |---------------------------------------------------------|
  |LAYOUT OPTIONS | fixed                                   |
  |               | layout-boxed                            |
  |               | layout-top-nav                          |
  |               | sidebar-collapse                        |
  |               | sidebar-mini                            |
  |---------------------------------------------------------|
  -->
  <body class="skin-blue sidebar-mini">
    <div class="wrapper">

      <!-- Main Header -->
      <% include commons/header.ejs %>
      <!-- Left side column. contains the logo and sidebar -->
     <% include commons/sidebar.ejs %>

      <!-- Content Wrapper. Contains page content -->
      <div class="content-wrapper">
        <!-- Content Header (Page header) -->
       

        <!-- Main content -->
        <section class="content">

         
          <div class="row">
            <div class="col-xs-12">
              <div class="nav-tabs-custom">
                <ul class="nav nav-tabs">
                  <li class="active"><a data-toggle="tab" href="#fa-icons">Post Event</a></li>
                  <li><a data-toggle="tab" href="#glyphicons">View Events</a></li>
                </ul>
                <div class="tab-content">
                  <!-- Font Awesome icons -->
                  <div id="fa-icons" class="tab-pane active">
                    <div class="row">
                      <div class="col-md-6">
                    <form id="post" >

                      <div class="form-group row">
                      <label for="classLevel" class="col-xs-3">CLass</label>
                       <div class="col-xs-6">
                       <select id="classLevel" name="classLevel" class="required">
                          <option value="">--select--</option>
                          <option value="I">I</option>
                          <option value="II">II</option>
                          <option value="III">III</option>
                          <option value="IV">IV</option>
                          <option value="V">V</option>
                          <option value="VI">VI</option>
                          <option value="VII">VII</option>
                          <option value="IIX">IIX</option>
                          <option value="IX">IX</option>
                          <option value="X">X</option>
                          <option value="All">All</option>
                       </select>
                    </div>
                    </div>

                      <div class="form-group row">
                      <label for="event date" class="col-xs-3">Event Date:</label>
                      <div class="col-xs-6">
                      <input type="text" placeholder="eventDate" class="required form-control" 
                       name="eventDate" autoComplete="off" >
                      
                      </div>
                    </div>

                    <div class="form-group row">
                      <label for="event date" class="col-xs-3">Start Time:</label>
                      <div class="col-xs-6">
                      <input type="text" placeholder="eventTime" class="required form-control" 
                       name="eventTime" autoComplete="off" >
                      
                      </div>
                    </div>

                      <div class="form-group row">
                      <label for="event Title" class="col-xs-3">Event Title:</label>
                      <div class="col-xs-6">
                      <input type="text" placeholder="Event Title"  class="required form-control" 
                       name="eventTitle" autoComplete="off" >
                      
                      </div>
                    </div>

                    <div class="form-group row">
                      <label for="event Location" class="col-xs-3">Event Location:</label>
                      <div class="col-xs-6">
                      <input type="text" placeholder="Event Location" class="required form-control" 
                       name="eventLocation" autoComplete="off" >
                      
                      </div>
                    </div>

                    <div class="form-group row">
                      <label for="event Location" class="col-xs-3">Event Description:</label>
                      <div class="col-xs-6">
                      <textarea placeholder="Description ..." rows="3" class="form-control required" name="eventDesc"></textarea> 
                     
                      </div>
                    </div>

                    <div class="row">
                      <div class="col-md-12" style="margin-left:17%">
                        <button type="reset" class="btn btn-primary">Reset</button>
                        <button type="button" class="btn btn-primary" name="submit" > Post</button>
                         <a class="btn btn-warning" id="spinner" style="display:none;">
                        <span class="glyphicon glyphicon-refresh spinning"></span> Wait...    
                        </a>
                      </div>
                    </div>
                    </form>
                    </div>
                    <div class="col-md-6" id="eventsByDate">
                      <!-- Events by date field change-->
                      
                      

                    </div>


                    </div>
                  </div><!-- /#fa-icons -->
                  <!-- glyphicons-->
                  <div id="glyphicons" class="tab-pane">

                    <div class="row">
                      

                      
                    </div>
                    
                  </div><!-- /#ion-icons -->

                </div><!-- /.tab-content -->
              </div><!-- /.nav-tabs-custom -->
            </div><!-- /.col -->
          </div><!-- /.row -->
       
        
        </section><!-- /.content -->
        
      </div><!-- /.content-wrapper -->

      <% include commons/footer.ejs %>
      
      
      <!-- Add the sidebar's background. This div must be placed
           immediately after the control sidebar -->
      <div class='control-sidebar-bg'></div>
    </div><!-- ./wrapper -->

    <!-- REQUIRED JS SCRIPTS -->

      <% include commons/scripts.ejs %>
      <script type="text/javascript" src="/bootstrap/js/jquery.dataTables.min.js"></script>
      <script type="text/javascript" src="/bootstrap/js/dataTables.bootstrap.min.js"></script>
       <script src="/lib/moment/min/moment.min.js" type="text/javascript"></script>
       <script src="/bootstrap/js/bootstrap-datetimepicker.min.js" type="text/javascript"></script>
       <script type="text/javascript" src="/events-script.js"></script>
       <script type="text/javascript">
       $(function(){
        $("#eventLi").addClass('active');
        $('input[name=eventDate]').datetimepicker({format: 'DD-MM-YYYY'});
        $('input[name=eventTime]').datetimepicker({
            format: "hh:mm A"
        });
       });
       </script>
       <script type="text/javascript">
$(window).load(function() {
  $(".loader").fadeOut("slow");
})
</script>
      </body>
</html>